#include <stdio.h>

int main()
{
	int bil[]={0};
	int n,a,b,max=bil[0],min;

	printf("Masukkan jumlah bilangan: ");
	scanf("%d",&b);
	
	for(a=0;a<b;a++){
	    printf("Masukkan bilangan ke-%d: ",a+1);
		scanf("%d",&n);
	
	    bil[a]=n;
	    if(bil[a]>max)
	        max=bil[a];
	    if(bil[a]<min)
	        min=bil[a];
		if(a==0)
	        min=bil[a];
	}
    printf("\nNilai Max= %d\n",max);
    printf("Nilai Min= %d\n",min);

return 0;
}
